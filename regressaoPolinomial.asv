function regressaoPolinomial(x, y, m)

    [i, j] = size(x);
    novoX = [x, sum(x)];
    novoY = [y, sumx(y)];
    xExp = zeros(i + 1, m + 1);
    xyExp = zeros(i + 1, m);
    
    for a=1:m+1
        for b=1:i
            xExp(b, a) = x(b, 1)^(a+1);
            
            if a <= m
                xyExp(b, a) = (x(b, 1)^a) * y(b, 1);
            end
        end
        xExp(b+1, a) = sum(xExp(1:b, a:a));
        xExp(b+1, a) = sum(xExp(1:b, a:a));
    end
        
    
end